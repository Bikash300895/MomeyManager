<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bikash.moneymanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/money"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FixedCost" />
        <activity android:name=".ExtraSpent" />
        <activity android:name=".AddMemo" />
        <activity android:name=".SignIn" />
        <activity android:name=".SetPassword" />
        <activity android:name=".History"></activity>
    </application>

</manifest>